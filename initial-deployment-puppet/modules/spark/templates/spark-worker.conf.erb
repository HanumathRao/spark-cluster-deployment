description "Spark Worker Service script"
start on runlevel [2345]
stop on runlevel [06]
#setuid root
#setgid root
#console log

chdir <%= @install_dir %>
exec <%= @install_dir %>/bin/spark-worker-runner.sh

# Try to respawn with a maximum of 10 times in a 90 second window.
respawn
respawn limit 10 90
